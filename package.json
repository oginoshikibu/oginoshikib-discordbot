    "tsx": "^4.19.2"
  },
  "peerDependencies": {
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "discord.js": "^14.16.3",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pm2": "^5.4.3",
    "zod": "^3.24.1"
